{
  "expo": {
    "name": "fun-kollection",
    "slug": "fun-kollection",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/missingfunko.png",
    "scheme": "missingfunkoapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cob3x85s.missingfunkoapp",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to let you select Funko Pop images for your collection.",
        "NSCameraUsageDescription": "This app needs access to your camera to let you take photos of your Funko Pops.",
        "NSPhotoLibraryAddUsageDescription": "This app needs access to save Funko Pop images to your photo library."
      }, 
      "icon": "./assets/images/missingfunko.png"
    },
    "android": {
      "icon": "./assets/images/missingfunko.png",
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.cob3x85s.missingfunkoapp",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.CAMERA"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "root": "./src/app"
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/missingfunko.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "image": "./assets/images/missingfunko.png",
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-sqlite",
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Fun-Kollection to access your photos to add Funko Pop images to your collection.",
          "cameraPermission": "Allow Fun-Kollection to use your camera to take photos of your Funko Pops."
        }
      ],
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "fun-collection",
          "organization": "carlos-ortiz-aa"
        }
      ],
      "expo-font",
      "expo-audio"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {
        "root": "./src/app"
      },
      "eas": {
        "projectId": "42470033-5d7b-4857-9b15-5d25cdce410b"
      }
    },
    "owner": "cob3x85s"
  }
}
